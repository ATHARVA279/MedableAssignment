{
  "name": "assessment-4-file-processing",
  "version": "1.0.0",
  "description": "File Upload/Processing API Assessment - Fix error handling and implement processing features",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "echo 'Build complete'",
    "test:cloudinary": "node test-cloudinary.js",
    "setup": "node test-cloudinary.js",
    "db:migrate": "node scripts/migrate-to-mongodb.js migrate",
    "db:health": "node scripts/migrate-to-mongodb.js health",
    "db:stats": "node scripts/migrate-to-mongodb.js stats",
    "backup:create": "node -e \"require('./utils/backupRecovery').backupRecovery.createFullBackup().then(console.log).catch(console.error)\"",
    "backup:list": "node -e \"require('./utils/backupRecovery').backupRecovery.listBackups().then(console.log).catch(console.error)\"",
    "health:check": "curl http://localhost:8888/health",
    "virus:test": "node -e \"require('./utils/virusScanner').virusScanner.getHealthStatus().then(console.log).catch(console.error)\""
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.12.2",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "file-type": "^16.5.4",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "mongoose": "^8.18.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.33.5",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "assessment",
    "api",
    "file-upload",
    "processing",
    "error-handling"
  ],
